      "url": "https://play.google.com/store/apps/details?id=com.mebelplace.app",
      "id": "com.mebelplace.app"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/mebelplace/id1234567890"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "capture_links": "existing-client-navigate",
  "protocol_handlers": [
    {
      "protocol": "web+mebelplace",
      "url": "/?action=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/upload",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".gif", ".webp"],
        "video/*": [".mp4", ".webm", ".mov", ".avi"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "image",
          "accept": ["image/*"]
        },
        {
          "name": "video",
          "accept": ["video/*"]
        }
      ]
    }
  },
  "features": [
    "Cross Platform",
    "fast",
    "simple"
  ],
