{
  "expo": {
    "name": "MebelPlace",
    "slug": "mebelplace-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/512x512.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/512x512.png",
      "resizeMode": "contain",
      "backgroundColor": "#f97316"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "mebelplace",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mebelplace.mobile",
      "icon": "./assets/180x180.png",
      "associatedDomains": ["applinks:mebelplace.com.kz"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/512x512.png",
        "backgroundColor": "#f97316"
      },
      "package": "com.mebelplace.mobile",
      "icon": "./assets/192x192.png",
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "mebelplace.com.kz",
              "pathPrefix": "/"
            },
            {
              "scheme": "mebelplace"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/48x48.png"
    },
    "plugins": [
      "expo-camera",
      "expo-av",
      [
        "expo-notifications",
        {
          "icon": "./assets/96x96.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "mebelplace-mobile-app"
      }
    }
  }
}
